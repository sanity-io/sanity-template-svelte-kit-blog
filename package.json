{
  "name": "sveltekit-sanity-template",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "prepare": "svelte-kit sync",
    "preview": "vite preview",
    "build:sanity": "cd studio && npm run build",
