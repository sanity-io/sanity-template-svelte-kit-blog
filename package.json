    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^2.8.4",
    "prettier-plugin-svelte": "^2.9.0",
    "svelte": "^3.55.1",
    "svelte-check": "^3.0.3",
    "typescript": "^4.9.5",
    "vite": "^4.1.1"
  },
  "type": "module",
  "dependencies": {
    "@sanity/client": "^5.2.1",
    "@sanity/image-url": "^1.0.1"
